{
  "name": "vue3-vite-template",
  "version": "0.0.0",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build-tsc": "vue-tsc --noEmit && vite build",
    "serve": "vite preview",
    "format": "prettier --write ./src",
    "lint": "eslint ./src --ext .vue,.js,.ts,.jsx,.tsx",
    "lint-fix": "eslint --fix ./src --ext .vue,.js,.ts,.jsx,.tsx",
    "test": "jest",
    "qat": "vite build --mode test",
    "release": "vite build --mode production",
    "gh-pages": "vite build --base=/vue3-vite-template/"
  },
  "dependencies": {
    "ant-design-vue": "^2.1.6",
    "axios": "^0.21.1",
    "vue": "^3.0.5",
    "vue-i18n": "^9.1.6",
    "vue-router": "^4.0.8",
    "vuex": "^4.0.0"
  },
  "devDependencies": {
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/nprogress": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^4.23.0",
    "@typescript-eslint/parser": "^4.23.0",
    "@vitejs/plugin-vue": "^1.2.2",
    "@vue/compiler-sfc": "^3.0.5",
    "@vue/test-utils": "^2.0.0-rc.6",
    "eslint": "^7.26.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.23.1",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-vue": "^7.9.0",
    "jest": "^26.6.3",
    "less": "^4.1.1",
    "moment": "^2.29.1",
    "nprogress": "^0.2.0",
    "prettier": "^2.3.0",
    "stylus": "^0.54.8",
    "ts-jest": "^26.5.6",
    "typescript": "^4.1.3",
    "vite": "^2.3.0",
    "vite-plugin-import": "^0.3.0",
    "vite-plugin-style-import": "^0.10.0",
    "vue-jest": "^5.0.0-alpha.9",
    "vue-tsc": "^0.0.24"
  }
}
